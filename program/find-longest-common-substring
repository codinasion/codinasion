#include<bits/stdc++.h>
using namespace std;

class Solution{
    public:
    
    int longestCommonSubstr (string S1, string S2, int n, int m)
    {
       
       int L[n+1][m+1];
       int ans =0;
       
       for(int i=0; i<n; i++){
           for(int j=0; j<m; j++){
               L[i][j] =0;
           }
       }
       
       for(int i=1; i<=n; i++){
           for(int j=1; j<=m; j++){
               if(S1[i-1]==S2[j-1]){
                   L[i][j] = L[i-1][j-1]+1;
                   ans = max(L[i][j], ans);
               }else {
                   L[i][j] =0;
               }
           }
       }
       return ans;
    }
};

int main()
{
    int t; cin >> t;
    while (t--)
    {
        int n, m; cin >> n >> m;
        string s1, s2;
        cin >> s1 >> s2;
        Solution ob;

        cout << ob.longestCommonSubstr (s1, s2, n, m) << endl;
    }
}
